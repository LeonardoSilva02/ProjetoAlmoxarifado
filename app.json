{
  "expo": {
    "name": "Almoxarifado MasterÂ´s",
    "slug": "ProjetoAlmoxarifado",
    "version": "1.0.0",
    "orientation": "portrait",

    "icon": "./assets/logo-masters.png",
    "userInterfaceStyle": "light",
    "scheme": "almoxarifado-masters",

    "newArchEnabled": true,

    "splash": {
      "image": "./assets/logo-masters.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b5394"
    },

    "android": {
      "package": "com.leonardosilva02.projetoalmoxarifado",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-masters.png",
        "backgroundColor": "#0b5394"
      },
      "permissions": [],
      "softwareKeyboardLayoutMode": "pan"
    },

    "ios": {
      "bundleIdentifier": "com.leonardosilva02.projetoalmoxarifado",
      "supportsTablet": false,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/logo-masters.png",
      "backgroundColor": "#ffffff",
      "meta": {
        "viewport": "width=device-width, initial-scale=1, maximum-scale=1"
      }
    },

    "updates": {
      "fallbackToCacheTimeout": 0
    },

    "plugins": [
      "expo-font",
      "expo-web-browser"
    ],

    "extra": {
      "eas": {
        "projectId": "e53629f2-e5fa-4623-ab5c-61907c1aa295"
      }
    }
  }
}
